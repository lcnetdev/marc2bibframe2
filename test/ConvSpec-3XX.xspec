    <x:expect label="$b creates an rdfs:label with prefix 'Textual assistive features:'" test="//bf:Work[1]/bf:contentAccessibility[1]/bf:ContentAccessibility/rdfs:label[2] = 'Textual assistive features: transcript'"/>
